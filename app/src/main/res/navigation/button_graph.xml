<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/button_graph"
    app:startDestination="@id/tthFragment">

    <fragment
        android:id="@+id/tthFragment"
        android:name="com.example.muirsuus.ui.layouts.TthFragment"
        tools:layout="@layout/fragment_layout_tth">
        <action
            android:id="@+id/action_navigation_second_level_of_nesting_to_navigation_Description"
            app:destination="@id/descriptionFragment" />
        <action
            android:id="@+id/action_secondLevelOfNesting_to_historyFragment"
            app:destination="@id/historyFragment" />
    </fragment>


    <fragment
        android:id="@+id/historyFragment"
        android:name="com.example.muirsuus.ui.layouts.HistoryFragment"
        tools:layout="@layout/fragment_layout_history" />
    <fragment
        android:id="@+id/descriptionFragment"
        android:name="com.example.muirsuus.ui.layouts.DescriptionFragment"
        tools:layout="@layout/fragment_layout_description"/>
</navigation>